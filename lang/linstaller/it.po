msgstr "È stato selezionato:"

#: linstaller/modules/partdisks/front.py:389
#: linstaller/modules/partdisks/front.py:1019
msgid "not formatted"
msgstr "non formattato"

#: linstaller/modules/partdisks/front.py:391
#: linstaller/modules/partdisks/front.py:1021
msgid "free space"
msgstr "spazio libero"

#: linstaller/modules/partdisks/front.py:394
#: linstaller/modules/partdisks/front.py:1024
msgid "extended"
msgstr "estesa"

#: linstaller/modules/partdisks/front.py:427
msgid "Add partition"
msgstr "Aggiungi partizione"

#: linstaller/modules/partdisks/front.py:430
#: linstaller/modules/partdisks/front.py:771
msgid "Format partition"
msgstr "Formatta partizione"

#: linstaller/modules/partdisks/front.py:431
#: linstaller/modules/partdisks/front.py:802
msgid "Delete partition"
msgstr "Elimina partizione"

#: linstaller/modules/partdisks/front.py:432
#: linstaller/modules/partdisks/front.py:848
msgid "Resize partition"
msgstr "Ridimensiona partizione"

#: linstaller/modules/partdisks/front.py:433
#: linstaller/modules/partdisks/front.py:820
msgid "Use as..."
msgstr "Usa come..."

#: linstaller/modules/partdisks/front.py:434
#: linstaller/modules/partdisks/front.py:449
#: linstaller/modules/partdisks/front.py:907
msgid "Unmark changes"
msgstr "Deseleziona modifiche"

#: linstaller/modules/partdisks/front.py:437
#: linstaller/modules/partdisks/front.py:450
#: linstaller/modules/partdisks/front.py:461
#: linstaller/modules/partdisks/front.py:605
#: linstaller/modules/partdisks/front.py:1102
msgid "<- Back"
msgstr "<- Indietro"

#: linstaller/modules/partdisks/front.py:448
msgid "Delete all partitions on the disk"
msgstr "Elimina tutte le partizioni del disco"

#: linstaller/modules/partdisks/front.py:460
