#: linstaller.py:260
msgid "You have to pass at least an action!"
msgstr "¡Ud tiene que pasar al menos una acción!"

#: linstaller.py:264
#, python-format
msgid "SYNTAX: %s <options> [ACTION] <:seed:option:value>"
msgstr "SINTAXIS: %s <options> [ACTION] <:seed:option:value>"

#: linstaller.py:266
msgid "Recognized options:"
msgstr "Opciones reconocidas:"

#: linstaller.py:267
msgid " -c|--config\t\t- Selects the configuration file to read"
msgstr " -c|--config\t\t- Selecciona el archivo de configuración a leer"

#: linstaller.py:268
msgid ""
" -p|--configpath\t- Selects the directory to look for configuration files"
msgstr " -p|--configpath\t- Selecciona el directorio para buscar archivos de configuración"

#: linstaller.py:269
msgid " -t|--target\t\t- Selects the target directory (def: /linstaller/target)"
msgstr " -t|--target\t\t- Selecciona el directorio de destino (def: /linstaller/target)"

#: linstaller.py:270
msgid " -f|--frontend\t\t- Selects the frontend to use (def: cli)"
msgstr " -f|--frontend\t\t- Selecciona la interfaz de usuario a usar (def: cli)"

#: linstaller.py:271
msgid " -m|--modules\t\t- Overrides the modules to be executed"
msgstr " -m|--modules\t\t- Anula los módulos a ejecutarse"

#: linstaller.py:272
msgid " -s|--services\t\t- Overrides the services to be executed"
msgstr " -s|--services\t\t- Anula los servicios a ejecutarse"

#: linstaller.py:273
msgid " -r|--remove\t\t- Removes the specified modules from the modules list"
msgstr " -r|--remove\t\t- Elimina los módulos especificados de la lista de módulos"

#: linstaller.py:275
msgid "Recognized actions:"
msgstr "Acciones reconocidas:"

#: linstaller.py:276
msgid " help\t\t\t- Displays this help message, then exits."
msgstr " help\t\t\t- Muestra este mensaje de ayuda y luego sale."

#: linstaller.py:277
msgid " start\t\t\t- Starts the installer."
msgstr " start\t\t\t- Inicia el instalador."

#: linstaller.py:279
msgid "Preseeding:"
msgstr ""

#: linstaller.py:280
msgid ""
" linstaller supports preseeding. Seeds can be specified into the\n"
" configuration file of the distribution, or with the notation below:\n"
" \n"
" :module:option:value\n"
" \n"
" Examples:\n"
" \n"
" :userhost:root:True\t\t- Enables root\n"
" :language:ask:True\t\t- Asks for language\n"
" :partdisks:swap_noformat:True\t- Does not format swap partition\n"
msgstr ""

